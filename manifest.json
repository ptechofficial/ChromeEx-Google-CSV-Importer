{
  "manifest_version": 2,
  "name": "EasyUploadCSV",
  "description": "Ease the process of importing csv to google sheets",
  "version": "1.0",
  "icons": {
    "48": "/images/icon48.png",
    "128": "/images/icon128.png"
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab", "fileBrowserHandler"]
}
